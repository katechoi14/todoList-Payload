{
  "name": "react.dev",
  "version": "0.0.0",
  "main": "/src/index.tsx",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "dev:payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts nodemon",
    "drop": "cross-env PAYLOAD_DROP_DATABASE=true PAYLOAD_CONFIG_PATH=src/payload.config.ts nodemon",
    "build:payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload build",
    "build:server": "tsc",
    "build:combined": "yarn copyfiles && yarn build:payload && yarn build:server",
    "syncToAlgolia": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js NODE_ENV=production node dist/syncToAlgolia.js",
    "serve": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js NODE_ENV=production node dist/server.js",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" dist/",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:types",
    "generate:graphQLSchema": "PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:graphQLSchema",
    "lint": "eslint src",
    "lint:fix": "eslint --fix --ext .ts,.tsx src",
    "payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload",
    "populate-ch": "ts-node src/scripts/populate-community-help.ts"
 
  },
  "dependencies": {
    "@types/react-router-dom": "^5.3.3",
    "autoprefixer": "^10.4.19",
    "axios": "^1.7.2",
    "devextreme": "23.2.6",
    "devextreme-react": "23.2.6",
    "flowbite": "^2.3.0",
    "flowbite-datepicker": "^1.2.7",
    "next": "^14.2.4",
    "payload": "^2.21.0",
    "postcss": "^8.4.38",
    "react": "^18.0.0",
    "react-datepicker": "^6.9.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.23.1"
  },
  "devDependencies": {
    "@catppuccin/tailwindcss": "^0.1.6",
    "@types/node": "20.14.2",
    "@types/react": "^18.3.3",
    "@types/react-datepicker": "^6.2.0",
    "@types/react-dom": "^18.3.0",
    "tailwindcss": "^3.4.4",
    "typescript": "5.4.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
